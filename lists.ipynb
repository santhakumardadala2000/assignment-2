   "source": [
    "def last(n):\n",
    "    return n[-1]\n",
    "def sort(tuples):\n",
    "    return sorted(tuples,key=last)\n",
    "sample_list=[(2,5),(1,2),(4,4),(2,3),(2,1)]\n",
    "print(\" sorted list: \")\n",
    "print(sort(sample_list))"
